#include <iostream>
#include <Windows.h>



using namespace std;
class Rectangl {
private:
	int length;
	int width;
public:
	
	void Set_length( int a){length=a;}
	void Set_width( int b){width=b;}
	int Get_length(){return length;}
	int Get_widht(){return width;}
	 
		int square(){int S;
		S=length*width;
		return S;}


		int perimeter(){int P;
		P=length+width;
		return P;
    }
